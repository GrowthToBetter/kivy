#:kivy 2.1.0
<analog>:
    Button:
        text: '>'
        size_hint: 0.1, 0.1
        pos_hint: {'x': 0.242, 'y': 0.12}
        background_color: (0.78, 0, 0.2, 1)
        on_press: self.background_color = (0.78, 0.2, 0.2, 1)
        on_release: self.background_color = (0.78, 0, 0.2, 1)
        on_press: root.move_r()
    Button:
        text:'<'
        size_hint: 0.1,0.1
        pos_hint: {'x': 0.015, 'y': 0.12}
        background_color: (0.78, 0, 0.2, 1)
        on_press: self.background_color = (0.78, 0.2, 0.2, 1)
        on_release: self.background_color = (0.78, 0, 0.2, 1)
    Button:
        text:'^'
        size_hint: 0.1,0.1
        pos_hint: {'x':0.128, 'y':0.20}
        background_color: (0.78, 0, 0.2, 1)
        on_press: self.background_color = (0.78, 0.2, 0.2, 1)
        on_release: self.background_color = (0.78, 0, 0.2, 1)
    Button:
        text:'v'
        size_hint: 0.1,0.1
        pos_hint: {'x':0.128,'y':0.028}
        background_color: (0.78, 0, 0.2, 1)
        on_press: self.background_color = (0.78, 0.2, 0.2, 1)
        on_release: self.background_color = (0.78, 0, 0.2, 1)

    Label:
        id: obj2
        text: ''
        size_hint: 0.1, 0.1
        pos_hint: {'x': 0, 'y': 0.5}
        background_color: (1, 1, 1, 1)
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1  # Warna latar belakang (misalnya merah)
            Rectangle:
                pos: self.pos
                size: self.size